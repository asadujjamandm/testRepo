
    <%= render 'calendar_scripts' %>
    <div class=" fc-header" >
      <div style="float: left;">
          <%= 
          unless @previous_calendar_date.nil?
            link_to '<span class="fc-button fc-button-prev fc-state-default fc-corner-left"> <i class="fa fa-caret-left"></i></span>'.html_safe, 'javascript:moveCalendar( "' + @previous_calendar_date + '" );', :title => 'Previous Month' 
          end 
          %>
      </div>
      <div style="float: right;">
      <%= 
      unless @next_calendar_date.nil?
      link_to '<span class="fc-button fc-button-next fc-state-default fc-corner-right"><i class="fa fa-caret-right"></i></span>'.html_safe, 'javascript:moveCalendar( "' + @next_calendar_date + '" );', :title => 'Next Month' 
      end 
      %>
      </div>
      <div style="text-align: center;">
        <span class="fc-header-title">
               <strong><%= @calendar_info['0'] unless @calendar_info['0'].nil? %> </strong>
        </span>
      </div>
    </div>
